            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <ImageView fitHeight="28.0" fitWidth="28.0" layoutX="5.0" layoutY="621.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/refresh.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="26.0" fitWidth="28.0" layoutX="416.0" layoutY="14.0" onMouseClicked="#exit" pickOnBounds="true">
