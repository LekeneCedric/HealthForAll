            <Image url="@images/logout.png" />
         </image>
      </ImageView>
      <Label alignment="CENTER" layoutX="31.0" layoutY="288.0" prefHeight="17.0" prefWidth="89.0" text="Description :" textFill="#769e71">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="adressImage" layoutX="120.0" layoutY="233.0" prefHeight="25.0" prefWidth="283.0" />
      <TextArea fx:id="descriptionTraitement" layoutX="120.0" layoutY="288.0" prefHeight="321.0" prefWidth="283.0" />
