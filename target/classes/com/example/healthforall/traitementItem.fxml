<VBox fx:id="traitementLayout" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.healthforall.TraitementItemController">
   <children>
      <HBox>
         <children>
            <AnchorPane prefHeight="213.0" prefWidth="754.0" style="-fx-border-color: #1b3f65; -fx-border-width: 5;">
               <children>
                  <ImageView fx:id="imagetraitement" fitHeight="131.0" fitWidth="178.0" layoutX="14.0" layoutY="73.0" pickOnBounds="true">
                     <image>
                        <Image url="@images/no-bugs.png" />
                     </image>
                  </ImageView>
                  <Label fx:id="nomtraitement" alignment="CENTER" layoutX="14.0" layoutY="20.0" prefHeight="52.0" prefWidth="178.0" text="PALUDISME" textFill="#163a57" wrapText="true">
